
@model SPSA.Autorizadores.Aplicacion.DTO.MaeCadenaDTO

<div class="row needs-validation" id="formCadena">
    <div class="col-xl-12" style="display: none;">
        <div class="form-group">
            <label for="txtCodEmpresa" class="form-label">Empresa</label>
            <input type="text" class="form-control form-control-sm" id="txtCodEmpresa" value="@Model.CodEmpresa" required disabled>
        </div>
    </div>
    <div class="col-xl-12">
        <div class="form-group">
            <label for="txtCodCadena" class="form-label">Codigo</label>
            <input type="text" class="form-control form-control-sm" id="txtCodCadena" value="@Model.CodCadena" required>
        </div>
    </div>
    <div class="col-xl-12">
        <div class="form-group">
            <label for="txtNomCadena" class="form-label">Nombre</label>
            <input type="text" class="form-control form-control-sm" id="txtNomCadena" value="@Model.NomCadena" required oninput="this.value = this.value.toUpperCase()">
        </div>
    </div>
    <div class="col-xl-12">
        <div class="form-group">
            <label for="txtCadNumero" class="form-label">CadNumero</label>
            <input type="number" class="form-control form-control-sm" id="txtCadNumero" value="@Model.CadNumero" required>
        </div>
    </div>
</div>